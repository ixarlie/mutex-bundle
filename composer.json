{
  "name": "ixarlie/mutex-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony bundle for Mutex implementation for PHP",
  "keywords": [
    "symfony",
    "bundle",
    "semaphore",
    "mutex",
    "locking",
    "lock",
    "atomicity",
    "atom",
    "acquire",
    "release",
    "redis",
    "memcached",
    "sem",
    "flock"
  ],
  "homepage": "https://github.com/ixarlie/mutex-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Dominguez",
      "email": "ixarlie@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.13 || ^8.0",
    "doctrine/annotations": "^1.13",
    "symfony/framework-bundle": "^4.0 || ^5.0",
    "symfony/lock": "^4.0 || ^5.0",
    "symfony/event-dispatcher": "^4.0 || ^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.5 || ^9.5",
    "symfony/security-core": "^4.0 || ^5.0"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "IXarlie\\MutexBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IXarlie\\MutexBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
