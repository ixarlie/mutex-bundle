{
  "name": "ixarlie/mutex-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony bundle for Mutex implementation for PHP",
  "keywords": ["Symfony2", "bundle", "semaphore", "mutex", "locking", "lock", "atomicity", "atom", "acquire", "release", "redis", "memcached", "sem", "flock"],
  "homepage": "https://github.com/ixarlie/mutex-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Dominguez",
      "email": "ixarlie@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/dependency-injection": "~2.3|3.*",
    "arvenil/ninja-mutex": "~0.6",
    "doctrine/common": "2.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "suggest": {
    "doctrine/common": "In order to use controller annotations, make sure you have it in your project's dependencies",
    "symfony/monolog-bundle": "Allows more advanced logging of the application flow"
  },
  "autoload": {
    "psr-0": { "IXarlie\\MutexBundle": "" }
  },
  "target-dir": "IXarlie/MutexBundle",
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
