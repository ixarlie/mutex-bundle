{
  "name": "ixarlie/mutex-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony bundle for Mutex implementation for PHP",
  "keywords": ["Symfony2", "bundle", "semaphore", "mutex", "locking", "lock", "atomicity", "atom", "acquire", "release", "redis", "memcached", "sem", "flock"],
  "homepage": "https://github.com/ixarlie/mutex-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Dominguez",
      "email": "ixarlie@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6|>=7.1",
    "symfony/lock": "^3.4|^4.1",
    "symfony/dependency-injection": "^3.4|^4.1",
    "symfony/config": "^3.4|^4.1",
    "symfony/http-kernel": "^3.4|^4.1",
    "doctrine/annotations": "^1.6",
    "sensio/framework-extra-bundle": "^5.2"
  },
  "require-dev": {
    "symfony/translation": "^3.4|^4.1",
    "symfony/security": "^3.4|^4.1",
    "phpunit/phpunit": "^6.0|^7.0",
    "symfony/yaml": "^3.4|^4.1"
  },
  "suggest": {
  },
  "autoload": {
    "psr-0": { "IXarlie\\MutexBundle": "" }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "target-dir": "IXarlie/MutexBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
